function mostrarPopup() {
  const popup = $(`
    <div style="
      position: fixed;
      bottom: 20px;
      right: 20px;
      background-color: white;
      border: 2px solid gold;
      border-radius: 5px;
      padding: 10px;
      display: flex;
      align-items: center;
      box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.25);
      opacity: 0;
      transition: opacity 0.5s;
      @media (max-width: 767px) {
        right: auto;
        left: 50%;
        transform: translateX(-50%);
      }
    ">
      <img decoding="async" src="http://pulsemax.com.br/wp-content/uploads/2024/02/5-POTES-DESFOCADO.png" style="
        width: 70px;
        height: 70px;
        border-radius: 50%;
        margin-right: 10px;
      ">
      <span style="
        font-size: 18px;
     
      ">${nomeAleatorio()} comprou o<br /> NervoCure</span>
    </div>
  `);
  
  $("body").append(popup);
  popup.css("opacity", "1");

  setTimeout(() => {
    popup.css("opacity", "0");
    setTimeout(() => popup.remove(), 500);
  }, 4000);
}

setInterval(mostrarPopup, 15000);
